ID Department (
	id,
	key parent,				-- root '-', root + name 构成大类。大类+name构成组，组+name构成tag
	key name TextId,		-- textID
);

OPEN IX DepartmentStaff (
	id,
	id2 log,
);

ID Position (
	id,
	KEY no,					-- 职位定义编号
	name TextID,			-- 职位名称
);

OPEN IX StaffPosition (
	id,						-- staff
	id2 log,				-- Position
);
